<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}RuneScape Utility{% endblock %}</title>
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>

    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('vendor/bootstrap-3.3.7-dist/css/bootstrap.css') }}">
        <link rel="stylesheet" href="{{ asset('vendor/DataTables-1.10.13/media/css/jquery.dataTables.css') }}">
    {% endblock %}

    {% block libraries %}
        <script type="text/javascript" src="{{ asset('js/jquery-3.1.1.js') }}"></script>
        <script type="text/javascript" src="{{ asset('vendor/bootstrap-3.3.7-dist/js/bootstrap.js') }}"></script>
        <script type="text/javascript"
                src="{{ asset('vendor/DataTables-1.10.13/media/js/jquery.dataTables.js') }}"></script>
    {% endblock %}
</head>
<body>
{% block body %}{% endblock %}
{% block javascripts %}{% endblock %}
</body>
</html>
